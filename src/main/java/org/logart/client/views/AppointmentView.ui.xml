<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui"
			 xmlns:c="urn:import:com.google.gwt.user.cellview.client"
		>
	<g:HorizontalPanel>
		<g:HTMLPanel>
			<g:Label text="start time"/>
			<g:TextBox ui:field="startTime"/>
			<g:Label text="start date"/>
			<g:TextBox ui:field="startDate"/>
			<g:Label text="end time"/>
			<g:TextBox ui:field="endTime"/>
			<g:Label text="end date"/>
			<g:TextBox ui:field="endDate"/>
			<g:Label text="description"/>
			<g:TextBox ui:field="description"/>
			<g:Label text="period"/>
			<g:ListBox ui:field="period"/>
			<g:Button ui:field="button">create appointment</g:Button>
		</g:HTMLPanel>
		<g:HTMLPanel>
			<c:CellTable
					pageSize='15'
					ui:field='table' width="800px" height="600px">
			</c:CellTable>
		</g:HTMLPanel>
	</g:HorizontalPanel>
</ui:UiBinder>
